#include <iostream>
#include <conio>

int main(){
	cout << " Program C++ Persegi Panjang " << endl;
   cout << "=============================" << endl;
   cout << endl;

   int besar_persegi, i, j;

   cout << "Masukkan besarnya persegi yang diinginkan: ";
   cin >> besar_persegi;
   cout << endl;

   for(i = 1; i <= besar_persegi; i++){
   	for(j = 1; j <= besar_persegi; j++){
      	cout << " *";
      }
   	cout << endl;
   }

   getch();
}
